global class DailyLeadProcessor implements Schedulable{
    global void execute (SchedulableContext sc){
        List<Lead> lstOfLead = [select id from lead where leadSource = null LIMIT 200];
        
        List<Lead> lstofUpdateLead = new List<Lead>();
      
          for(Lead led: lstOfLead)
          {
              led.LeadSource = 'Dreamforce';
              lstofUpdateLead.add(led);
          }
          update lstofUpdateLead;
        
        
    }

}
